				<State Name="MarketaDied" PositionY="-80" PositionX="340" TypeT="bool">
					<Edge From="deathtrigger1.OnDeath" To="SetTrue" />
					<Edge From="ifdeathcheck_marketa.True" To="SetTrue" />
