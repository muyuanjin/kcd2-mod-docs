						<EnumLog Type="Started" Name="Active">
							<Log StringName="vyrob_pod_ted_kdyz_vim_ja_3qUM" Text="Teď, když vím jak na to, můžu vyrobit Jankovi jeho vlastní sadu podkov s ozuby." />
						</EnumLog>
						<EnumLog Type="Completed" Name="Done">
