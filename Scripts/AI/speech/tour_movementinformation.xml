															<Sequence>
																<ChangeFormation NewName="$formationName" Formation="$formationWUID" />
																<Expression expressions="$currentFormation = $formationName" />
															</Sequence>
														</IfGate>
													</While>
												</Parallel>
											</Sequence>
										</Subtree>
										<Cleanup canSkip="1">
